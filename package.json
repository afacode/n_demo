{
  "name": "node_nest_api",
  "version": "0.0.1",
  "description": "nest rest api",
  "author": "afacode",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "cross-env RUN_ENV=prod nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "cross-env NODE_ENV=development RUN_ENV=dev nest start",
    "start:dev": "cross-env NODE_ENV=development RUN_ENV=dev nest start --watch ",
    "start:debug": "nest start RUN_ENV=dev --debug --watch",
    "start:prod": "node dist/apps/node_nest_api/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/node_nest_api/test/jest-e2e.json",
    "typeorm": "ts-node ./node_modules/typeorm/cli",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:generate": "npm run typeorm -- migration:generate -d ./src/data-source.ts",
    "migration:run": "npm run typeorm -- migration:run -d ./src/data-source.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d ./src/data-source.ts"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.10.11",
    "@grpc/proto-loader": "^0.7.13",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^10.3.10",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.2.7",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/typeorm": "^10.0.0",
    "@nestjs/websockets": "^10.2.7",
    "@types/bcrypt": "^5.0.1",
    "axios": "^1.6.2",
    "base62": "^2.0.1",
    "bcrypt": "^5.1.1",
    "bull": "^4.11.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "compressing": "^1.10.0",
    "cors": "^2.8.5",
    "cron": "^3.1.4",
    "cron-parser": "^4.9.0",
    "cross-env": "^7.0.3",
    "crypto-js": "^4.2.0",
    "express": "^4.19.2",
    "lodash": "^4.17.21",
    "multer": "1.4.5-lts.1",
    "mysql2": "^3.6.2",
    "nest-winston": "^1.9.4",
    "nestjs-knife4j": "^1.0.1",
    "node-disk-info": "^1.3.0",
    "redis": "^4.6.10",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rxjs": "^7.8.1",
    "socket.io": "^4.7.2",
    "svg-captcha": "^1.4.0",
    "swagger-ui-express": "^5.0.0",
    "systeminformation": "^5.21.17",
    "typeorm": "^0.3.17",
    "ua-parser-js": "^1.0.37",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bull": "^4.10.0",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.9",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@types/ua-parser-js": "^0.7.39",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/lib_base(|/.*)$": "<rootDir>/libs/lib_base/src/$1"
    }
  }
}